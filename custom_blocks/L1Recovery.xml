<?xml version="1.0"?>
<block>
  <name>L1Recovery</name>
  <key>custom_L1Recovery</key>
  <category>Custom</category>
  <import>from custom_blocks import L1Recovery</import>
  <make>L1Recovery.L1Recovery($high_rate, $low_rate)</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
  <param>
    <name>high_rate</name>
    <key>high_rate</key>
    <type>int</type>
  </param>

  <param>
    <name>low_rate</name>
    <key>low_rate</key>
    <type>int</type>
  </param>

  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>in</name>
    <type>float</type>
    <vlen>$low_rate</vlen>
  </sink>
  <sink>
    <name>in2</name>
    <type>float</type>
    <vlen>$high_rate</vlen>
  </sink>

  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>out</name>
    <type>float</type>
    <vlen>$high_rate</vlen>
  </source>
</block>
